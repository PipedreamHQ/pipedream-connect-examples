# Required: Server-side OAuth credentials (available in your project settings)
PIPEDREAM_CLIENT_ID=<your-oauth-client-id>
PIPEDREAM_CLIENT_SECRET=<your-oauth-client-secret>
PIPEDREAM_PROJECT_ID=<your-project-id>  # Starts with 'proj_', available in the Pipedream UI
PIPEDREAM_PROJECT_ENVIRONMENT=development
PIPEDREAM_ALLOWED_ORIGINS='["https://example.com", "http://localhost:3000", "http://localhost:3001"]'

# Required: Client-side project environment (should match PIPEDREAM_PROJECT_ENVIRONMENT)
NEXT_PUBLIC_PIPEDREAM_PROJECT_ENVIRONMENT=development

# Optional: Set a static external user ID for easier local debugging
NEXT_PUBLIC_EXTERNAL_USER_ID=test-user-abc-123
